{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "withreact": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/withreact",
      "sourceRoot": "projects/withreact/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "allowedCommonJsDependencies": [  "react-dom/client" ],
            "outputPath": "dist/withreact",
            "index": "projects/withreact/src/index.html",
            "main": "projects/withreact/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/withreact/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/withreact/src/favicon.ico",
              "projects/withreact/src/assets"
            ],
            "styles": [
              "projects/withreact/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "withreact:build:production"
            },
            "development": {
              "browserTarget": "withreact:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "withreact:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/withreact/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/withreact/src/favicon.ico",
              "projects/withreact/src/assets"
            ],
            "styles": [
              "projects/withreact/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    },
